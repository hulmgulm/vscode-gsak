{
  "comments": {
    // symbol used for single line comment. Remove this entry if your language does not support line comments
    "lineComment": "#"
  },
  // symbols used as brackets
  "brackets": [
    ["(", ")"]
  ],
  // symbols that are auto closed when typing
  "autoClosingPairs": [
    ["(", ")"],
    ["\"", "\""],
    ["If", "EndIf"],
    ["IF", "ENDIF"],
    ["BeginCase", "EndCase"],
    ["BEGINCASE", "ENDCASE"],
    ["BeginSub", "EndSub"],
    ["BEGINSUB", "ENDSUB"],
    ["FileRead", "EndRead"],
    ["FILEREAD", "ENDREAD"],
    ["Repeat", "Until"],
    ["REPEAT", "UNTIL"],
    ["While", "EndWhile"],
    ["WHILE", "ENDWHILE"],
    ["<Data>", "<EndData>"]
  ],
  // symbols that that can be used to surround a selection
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""]
  ]
}
